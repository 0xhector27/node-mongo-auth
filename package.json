{
  "name": "webapp-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www",
    "prod": "NODE_ENV=production npm start",
    "dev": "DEBUG=webapp-backend:* npm run watch",
    "test": "NODE_ENV=test mocha ./test/index.js --exit",
    "windows:prod": "set NODE_ENV=production && npm start",
    "windows:dev": "set DEBUG=webapp-backend:* && npm run watch",
    "windows:test": "set NODE_ENV=test && mocha ./test/index.js --exit",
    "lint": "eslint ./ --fix"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "datejs": "^1.0.0-rc3",
    "debug": "^4.1.1",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-jwt-blacklist": "^1.1.0",
    "express-jwt-permissions": "^1.3.2",
    "helmet": "^3.21.2",
    "http-errors": "^1.7.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.3",
    "morgan": "^1.9.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  }
}
